private PacMan pacman;
private int gridSize = 30;

void setup() {
  size(780, 870);
  pacman = new PacMan();
}

void draw() {
  //Perform actions
  runTick();

  //Draw to screen
  background(255, 255, 255);

  //Draw walls and food
  for (int r = 0; r <board.length; r++) {
    for (int c = 0; c<board[0].length; c++) {
      if (board[r][c] == 2) {
        circle(gridSize*c+gridSize/2, gridSize*r+ gridSize/2, gridSize/2);
      } else if (board[r][c] == 1) {
        circle(gridSize*c+gridSize/2, gridSize*r+ gridSize/2, gridSize/3);
      } else if(board[r][c]==0){
        rect(gridSize*c, gridSize*r, gridSize, gridSize);
      }
    }
  }
  //Draw pacman
  pacman.draw();
}

void keyPressed() {
  if (key == CODED) {
    if (keyCode == UP) {
      pacman.queueTurn(-.1, 0);
    } else if (keyCode == DOWN) {
      pacman.queueTurn(.1, 0);
    } else if (keyCode == LEFT) {
      pacman.queueTurn(0, -.1);
    } else if (keyCode == RIGHT) {
      pacman.queueTurn(0, .1);
    }
  }
}
void runTick() {
  pacman.move();
}
